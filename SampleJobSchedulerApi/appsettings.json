{
  "SourceOptions": {
    "TechnologiesUrl": "https://app.ifs.aero/EternalBlue/api/technologies",
    "CandidatesUrl": "https://app.ifs.aero/EternalBlue/api/candidates",
    "TechnologiesFile": "Data\\Technologies.json",
    "CandidatesFile": "Data\\Candidates.json"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "AllowedHosts": "*"
}
